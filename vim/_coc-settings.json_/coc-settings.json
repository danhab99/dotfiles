  "hover.target": "float",
  "suggest.autoTrigger": "always",
  "java.completion.guessMethodArguments": false,
  "inlayHint.enableParameter": false,
  "java.completion.enabled": true,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-18",
      "path": "/usr/lib/jvm/jdk-18.0.2.1",
      "default": true
    },
  ],
  "java.project.sourcePaths": [
    "/home/dan/Lime/tradedesk-rest"
  ],
  "java.errors.incompleteClasspath.severity": "ignore",
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "enable": true,
    }
  },
  "tailwindCSS.cssLanguages": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "stylus",
    "vue",
    "typescriptreact",
  ],
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact"
  ],
  "tailwindCSS.emmetCompletions": true,
  "java.import.gradle.java.home": "/usr/lib/jvm/jdk-15.0.2",
  "yaml.schemas": {
    "https://raw.githubusercontent.com/docker/compose/master/compose/config/config_schema_v2.0.json": "**/docker-compose*.yml"
  }
